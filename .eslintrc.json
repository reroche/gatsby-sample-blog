{
  "extends": ["airbnb", "@cherihung/gatsby"],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": [0],
    "react/require-default-props": [0],
    "global-require": [0],
    "import/no-extraneous-dependencies": [0],
    "jsx-a11y/anchor-is-valid": [0],
    "anchor-is-valid": [0],
    "no-undef": [0],
    "no-unused-vars": [1],
    "react/no-danger": [0],
    "camelcase": [0]
  }
}
